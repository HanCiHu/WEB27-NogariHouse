    "@routes/*": "src/api/routes/*",
    "@config/*": "src/config/*",
    "@interfaces/*": "src/interfaces/*",
    "@loaders/*": "src/loaders/*",
    "@models/*": "src/models/*",
    "@services/*": "src/services/*",
    "@subscribers/*": "src/subscribers/*",
    "@types/*": "src/types/*",
    "@utils/*": "src/utils/*"
